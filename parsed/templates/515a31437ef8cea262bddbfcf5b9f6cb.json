{"components/products/product-thumbnails":"{{#if this.length \">\" 1}}\n  <div class=\"product-thumbnails\">\n    {{#each this}}\n      <span\n        class=\"product-thumbnail {{#if @first}}active{{/if}}\"\n        data-image-thumb\n        data-high-res=\"{{getImage this 'large'}}\"\n        data-thumb-num=\"{{@index}}\"\n      >\n        <img\n          src=\"{{getImage this 'thumbnail'}}\"\n          alt=\"{{this.alt}}\"\n          title=\"{{this.alt}}\"\n        >\n      </span>\n    {{/each}}\n  </div>\n{{/if}}\n"}
