{"components/faceted-search/facets/rating":"<ul class=\"facet-option facet-rating {{#if selected}}is-open{{/if}} {{#unless start_collapsed}}is-open{{/unless}}\" data-facet-filter-wrapper>\n  {{#each items}}\n    <li data-facet-item>\n      <a class=\"facet-select {{#if selected}}checked{{/if}}\" href=\"{{url}}\" rel=\"nofollow\" data-faceted-search-facet>\n        {{> components/products/ratings rating=param_value}}\n        {{#if ../show_product_counts}}<span data-facet-item-count>({{count}})</span>{{/if}}\n      </a>\n    </li>\n  {{/each}}\n</ul>\n","components/products/ratings":"\n<div class=\"product-rating\">\n  <div class=\"rating-stars-readonly\" aria-hidden=\"true\">\n    {{#for 1 5}}\n      <i class=\"rating-star {{#if ../rating '>=' $index}}star-full{{else}}star-empty{{/if}}\"></i>\n    {{/for}}\n  </div>\n  <span class=\"rating-total-score sr-only\">{{this.rating}}</span>\n  {{#if aggregate}}\n    <a class=\"rating-total-votes\" href=\"#user-reviews\">({{product.num_reviews}})</a>\n  {{/if}}\n</div>\n"}
