{"components/faceted-search/facets/range":"<form class=\"facet-option facet-range {{#if selected}}is-open{{/if}} {{#unless start_collapsed}}is-open{{/unless}}\" method=\"get\" data-facet-filter-wrapper data-faceted-search-range>\n  {{#each current_selected_items}}\n    <input type=\"hidden\" name=\"{{param_name}}\" value=\"{{param_value}}\"/>\n  {{/each}}\n\n  <div class=\"form-field\">\n    <input\n      class=\"form-input min-price\"\n      name=\"min_price\"\n      min=\"0\"\n      type=\"number\"\n      value=\"{{min_price}}\"\n      placeholder=\"{{lang 'search.faceted.range.min_placeholder'}}\"\n      required\n    />\n  </div>\n\n  <div class=\"form-field\">\n    <input\n      class=\"form-input max-price\"\n      name=\"max_price\"\n      min=\"0\"\n      type=\"number\"\n      value=\"{{max_price}}\"\n      placeholder=\"{{lang 'search.faceted.range.max_placeholder'}}\"\n      required\n    />\n  </div>\n\n  <div class=\"form-actions\">\n    <input class=\"button\" type=\"submit\" value=\"{{lang 'search.faceted.range.update'}}\">\n  </div>\n\n</form>\n"}
