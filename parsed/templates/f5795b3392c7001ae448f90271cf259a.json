{"core/forms/common/number":"\n<div class=\"form-field form-field-common form-field-number {{#if required}} form-required{{/if}}\" id=\"{{id}}\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{label}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <input\n        class=\"form-input\"\n        id=\"input-{{id}}\"\n        name=\"{{name}}\"\n        type=\"number\"\n        value=\"{{value}}\"\n        min=\"{{limitfrom}}\"\n        max=\"{{limitto}}\"\n        title=\"{{lang 'forms.range' limitFrom=limitfrom limitTo=limitto}}\"\n        {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\">\n    </span>\n  </label>\n</div>\n"}
