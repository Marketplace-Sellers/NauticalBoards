{"components/common/newsletter-subscribe":"<form class=\"form subscription-form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\n  <div class=\"form-field form-inline subscription-form-email\">\n    <label class=\"form-label\" for=\"newsletter-subscribe\">{{lang 'newsletter.subscribe'}}</label>\n    <input class=\"form-input\" id=\"newsletter-subscribe\" type=\"email\" name=\"nl_email\" value=\"{{customer.email}}\" placeholder=\"{{lang 'newsletter.email_placeholder'}}\">\n\n    {{! TODO: Make sure the name/check fields are needed}}\n    <input type=\"hidden\" name=\"action\" value=\"subscribe\">\n    <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\n    <input type=\"hidden\" name=\"check\" value=\"1\">\n\n    <input class=\"button subscription-form-submit\" type=\"submit\" value=\"{{lang 'newsletter.subscribe_submit'}}\">\n  </div>\n</form>\n"}
