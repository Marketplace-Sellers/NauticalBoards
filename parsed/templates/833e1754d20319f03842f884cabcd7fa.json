{"core/forms/common/date":"\n<div class=\"form-field form-field-common form-field-date {{#if required}} form-required{{/if}}\" id=\"{{id}}\">\n  <span class=\"form-label\">\n\n    <span class=\"form-field-title\">\n      {{label}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n\n    <span class=\"form-field-control\">\n      <span class=\"form-select-wrapper form-date-year\">\n        <select\n          class=\"form-input form-select\"\n          id=\"date-year-{{id}}\"\n          name=\"{{year.name}}\"\n          {{#if required}}required{{/if}}\n          aria-required=\"{{required}}\">\n          <option disabled>{{lang 'forms.date.year'}}</option>\n          {{#each year.items}}\n            <option {{#if ../year.value '==' value}}selected{{/if}} value=\"{{value}}\">{{label}}</option>\n          {{/each}}\n        </select>\n      </span>\n\n      <span class=\"form-select-wrapper form-date-month\">\n        <select\n          class=\"form-input form-select\"\n          id=\"date-month-{{id}}\"\n          name=\"{{month.name}}\"\n          {{#if required}}required{{/if}}\n          aria-required=\"{{required}}\">\n          <option disabled>{{lang 'forms.date.month'}}</option>\n          {{#each month.items}}\n            <option {{#if ../month.value '==' value}}selected{{/if}} value=\"{{value}}\">{{label}}</option>\n          {{/each}}\n        </select>\n      </span>\n\n      <span class=\"form-select-wrapper form-date-day\">\n        <select\n          class=\"form-input form-select\"\n          id=\"date-day-{{id}}\"\n          name=\"{{day.name}}\"\n          {{#if required}}required{{/if}}\n          aria-required=\"{{required}}\">\n          <option disabled>{{lang 'forms.date.day'}}</option>\n          {{#each day.items}}\n            <option {{#if ../day.value '==' value}}selected{{/if}} value=\"{{value}}\">{{label}}</option>\n          {{/each}}\n        </select>\n      </span>\n    </span>\n\n  </span>\n</div>\n"}
