{"components/faceted-search/facets/hierarchy":"<div class=\"collection-facet-list-wrapper\n  facet-option\n  {{#if selected}}is-open{{/if}}\n  {{#unless start_collapsed}}is-open{{/unless}}\n  {{#if items_count '<' total_items}}has-additional-filters{{/if}}\"\n  data-facet-filter-wrapper>\n  <ul\n    class=\"facet-default\"\n    data-facet-name=\"{{facet}}\"\n    data-count=\"{{items_count}}\"\n    data-has-more-results=\"{{has_more_results}}\">\n    {{#each items}}\n      <li data-facet-item>\n        <a class=\"facet-select {{#if selected}}checked{{/if}}\" href=\"{{url}}\" rel=\"nofollow\" data-faceted-search-facet>\n          {{title}}\n          {{#if ../show_product_counts}}<span data-facet-item-count>({{count}})</span>{{/if}}\n        </a>\n      </li>\n    {{/each}}\n  </ul>\n\n  {{#if show_more_toggle}}\n    <div class=\"collection-facet-list-item collection-facet-list-separation\" data-facet-more>\n      <button\n        class=\"collection-facet-list-toggle\"\n        role=\"button\"\n        data-additional-facets=\"{{facet}}\">\n        <span class=\"show-more-text\">{{lang 'search.faceted.show_more'}}</span>\n        <span class=\"show-less-text\">{{lang 'search.faceted.show_less'}}</span>\n      </button>\n    </div>\n  {{/if}}\n</div>\n","components/faceted-search/facets/multi":"<div class=\"collection-facet-list-wrapper\n  facet-option\n  {{#if selected}}is-open{{/if}}\n  {{#unless start_collapsed}}is-open{{/unless}}\n  {{#if items_count '<' total_items}}has-additional-filters{{/if}}\"\n  data-facet-filter-wrapper>\n  <ul\n    class=\"facet-default\"\n    data-facet-name=\"{{facet}}\"\n    data-count=\"{{items_count}}\"\n    data-has-more-results=\"{{has_more_results}}\">\n    {{#each items}}\n      <li data-facet-item>\n        <a class=\"facet-select {{#if selected}}checked{{/if}}\" href=\"{{url}}\" rel=\"nofollow\" data-faceted-search-facet>\n          {{title}}\n          {{#if ../show_product_counts}}<span data-facet-item-count>({{count}})</span>{{/if}}\n        </a>\n      </li>\n    {{/each}}\n  </ul>\n\n  {{#if show_more_toggle}}\n    <div class=\"collection-facet-list-item collection-facet-list-separation\" data-facet-more>\n      <button\n        class=\"collection-facet-list-toggle\"\n        role=\"button\"\n        data-additional-facets=\"{{facet}}\">\n        <span class=\"show-more-text\">{{lang 'search.faceted.show_more'}}</span>\n        <span class=\"show-less-text\">{{lang 'search.faceted.show_less'}}</span>\n      </button>\n    </div>\n  {{/if}}\n</div>\n","components/faceted-search/facets/product-listing":"{{#if category.products}}\n  <form class=\"form\" action=\"/compare\" method='POST'>\n    {{#each category.products}}\n      {{name}}\n    {{/each}}\n  </form>\n{{else}}\n  <div>\n    {{lang 'category.empty'}}\n  </div>\n{{/if}}\n","components/faceted-search/facets/range":"<form class=\"facet-option facet-range {{#if selected}}is-open{{/if}} {{#unless start_collapsed}}is-open{{/unless}}\" method=\"get\" data-facet-filter-wrapper data-faceted-search-range>\n  {{#each current_selected_items}}\n    <input type=\"hidden\" name=\"{{param_name}}\" value=\"{{param_value}}\"/>\n  {{/each}}\n\n  <div class=\"form-field\">\n    <input\n      class=\"form-input min-price\"\n      name=\"min_price\"\n      min=\"0\"\n      type=\"number\"\n      value=\"{{min_price}}\"\n      placeholder=\"{{lang 'search.faceted.range.min_placeholder'}}\"\n      required\n    />\n  </div>\n\n  <div class=\"form-field\">\n    <input\n      class=\"form-input max-price\"\n      name=\"max_price\"\n      min=\"0\"\n      type=\"number\"\n      value=\"{{max_price}}\"\n      placeholder=\"{{lang 'search.faceted.range.max_placeholder'}}\"\n      required\n    />\n  </div>\n\n  <div class=\"form-actions\">\n    <input class=\"button\" type=\"submit\" value=\"{{lang 'search.faceted.range.update'}}\">\n  </div>\n\n</form>\n","components/faceted-search/facets/rating":"<ul class=\"facet-option facet-rating {{#if selected}}is-open{{/if}} {{#unless start_collapsed}}is-open{{/unless}}\" data-facet-filter-wrapper>\n  {{#each items}}\n    <li data-facet-item>\n      <a class=\"facet-select {{#if selected}}checked{{/if}}\" href=\"{{url}}\" rel=\"nofollow\" data-faceted-search-facet>\n        {{> components/products/ratings rating=param_value}}\n        {{#if ../show_product_counts}}<span data-facet-item-count>({{count}})</span>{{/if}}\n      </a>\n    </li>\n  {{/each}}\n</ul>\n","components/products/ratings":"\n<div class=\"product-rating\">\n  <div class=\"rating-stars-readonly\" aria-hidden=\"true\">\n    {{#for 1 5}}\n      <i class=\"rating-star {{#if ../rating '>=' $index}}star-full{{else}}star-empty{{/if}}\"></i>\n    {{/for}}\n  </div>\n  <span class=\"rating-total-score sr-only\">{{this.rating}}</span>\n  {{#if aggregate}}\n    <a class=\"rating-total-votes\" href=\"#user-reviews\">({{product.num_reviews}})</a>\n  {{/if}}\n</div>\n","components/search/sidebar":"<div class=\"faceted-search\" data-faceted-search data-search-query=\"{{forms.search.query}}\">\n  <h2 class=\"facet-heading\">{{lang 'search.faceted.selected.title'}}</h2>\n\n  {{#each product_results.facets}}\n    {{#if visible}}\n      <div class=\"facet-filter\" data-facet-filter>\n\n        <h3 class=\"facet-section-heading\">\n          {{title}}\n\n          {{#if remove_url}}\n            <a class=\"facet-clear\" href=\"{{remove_url}}\" data-faceted-search-facet data-facet-remove>{{lang 'search.faceted.clear'}}</a>\n          {{/if}}\n\n          {{#unless selected }}\n            {{#if start_collapsed}}\n              <span class=\"facet-toggle is-open\" data-facet-toggle>&#43;</span>\n            {{else}}\n              <span class=\"facet-toggle\" data-facet-toggle>&ndash;</span>\n            {{/if}}\n          {{/unless}}\n        </h3>\n\n        {{{dynamicComponent 'components/faceted-search/facets'}}}\n      </div>\n    {{/if}}\n  {{/each}}\n</div>\n"}
