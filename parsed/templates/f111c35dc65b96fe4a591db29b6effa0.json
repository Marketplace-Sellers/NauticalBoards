{"core/alerts/alert-error":"<div class=\"alert alert-error\">\n  {{#if dismissable}}<a class=\"alert-dismiss\">&times;</a>{{/if}}\n\n  <div class=\"alert-message\">\n    {{#if message}}\n      {{{message}}}\n    {{else}}\n      {{{this}}}\n    {{/if}}\n  </div>\n</div>\n","core/forms/common/text":"\n<div class=\"form-field form-field-common form-field-text {{#if required}} form-required{{/if}}\" id=\"{{id}}\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{label}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <input class=\"form-input\"\n        id=\"input-{{id}}\"\n        name=\"{{name}}\"\n        type=\"text\"\n        placeholder=\"{{prefill}}\"\n        {{#if value}}value=\"{{value}}\"{{/if}}\n        {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\">\n    </span>\n  </label>\n</div>\n","core/gift-certificate/balance":"<div class=\"giftcard-body\">\n  <div class=\"giftcard-heading-container\">\n    <h1 class=\"giftcard-heading\">{{lang 'core.giftcard.balance.heading'}}</h1>\n    <p class=\"giftcard-subheading\">{{lang 'core.giftcard.balance.subheading'}}</p>\n  </div>\n\n  <div class=\"giftcard-balance-container\">\n    {{#if gift_certificate}}\n      {{#if gift_certificate.error}}\n        {{> core/alerts/alert-error gift_certificate.error}}\n      {{else}}\n        <h2>{{gift_certificate.balance.formatted}}</h2>\n        <h3>{{gift_certificate.code}}</h3>\n      {{/if}}\n    {{/if}}\n\n    <form class=\"form form-giftcard-balance\" id=\"giftcard-balance\" method=\"get\" data-validated-form>\n      <input type=\"hidden\" name=\"action\" value=\"balance\">\n\n      {{> core/forms/common/text\n          required=true id=\"giftcard-code\"\n          name=\"giftcertificatecode\"\n          label=(lang 'core.giftcard.balance.code')\n          prefill=(lang 'core.giftcard.balance.code')\n      }}\n\n      <div class=\"form-actions\">\n        <input type=\"submit\" class=\"button button-primary\" value=\"{{lang 'core.giftcard.balance.submit_value'}}\">\n      </div>\n    </form>\n  </div>\n</div>\n"}
