{"components/cart/shipping-quotes":"\n<form class=\"form\">\n  <div class=\"form-field form-field-options form-field-radio\">\n    <div class=\"form-field-control\">\n      {{#if quotes.shipping_methods}}\n        {{#each quotes.shipping_methods}}\n          <label class=\"form-label\" for=\"shipping-quote-{{id}}\">\n            <input id=\"shipping-quote-{{id}}\" class=\"form-input form-radio\" type=\"radio\" name=\"shipping_method\" value=\"{{id}}\" data-shipping-quote>\n            <span class=\"form-label-text\">{{provider_name}} {{cost.formatted}}</span>\n          </label>\n        {{/each}}\n      {{/if}}\n      {{#if quotes.ups_shipping_methods}}\n        {{#each quotes.ups_shipping_methods}}\n            <label class=\"form-label\" for=\"shipping-quote-{{id}}\">\n              <input id=\"shipping-quote-{{id}}\" class=\"form-input form-radio\" type=\"radio\" name=\"shipping_method\" value=\"{{id}}\" data-shipping-quote>\n              <span class=\"form-label-text\">\n                {{#if logo_path}}<img class=\"ups-shipping-badge\" src=\"{{logo_path}}\" alt=\"\"/>{{/if}}\n                {{description}} {{cost.formatted}}\n              </span>\n            </label>\n        {{/each}}\n      {{/if}}\n    </div>\n  </div>\n  <div class=\"form-actions\">\n    <input class=\"button\" type=\"submit\" value=\"{{lang 'cart.shipping_calculator.select'}}\">\n  </div>\n</form>\n"}
