{"core/products/event":"<div class=\"form-field form-field-date form-required\">\n  <span class=\"form-label\">\n\n    <span class=\"form-field-title\">\n      {{product.event_date.name}}\n      <span class=\"required-text\">{{lang 'common.required'}}</span>\n    </span>\n\n    <span class=\"form-field-control\">\n      <span class=\"form-select-wrapper form-date-year\">\n        <select class=\"form-input form-select\" name=\"EventDate[Yr]\" required>\n          <option disabled selected>{{lang 'forms.date.year'}}</option>\n          {{#if product.event_date.earliest_year '==' product.event_date.latest_year}}\n            <option value=\"{{product.event_date.earliest_year}}\">{{product.event_date.earliest_year}}</option>\n          {{else}}\n            {{#for product.event_date.earliest_year product.event_date.latest_year}}\n              <option value=\"{{$index}}\">{{$index}}</option>\n            {{/for}}\n          {{/if}}\n        </select>\n      </span>\n\n      <span class=\"form-select-wrapper form-date-month\">\n        <select class=\"form-input form-select\" name=\"EventDate[Mth]\" required>\n          <option disabled>{{lang 'forms.date.month'}}</option>\n          {{#for 1 12}}\n            <option value=\"{{$index}}\">{{getShortMonth $index}}</option>\n          {{/for}}\n        </select>\n      </span>\n\n      <span class=\"form-select-wrapper form-date-day\">\n        <select class=\"form-input form-select\" name=\"EventDate[Day]\" required>\n          <option disabled>{{lang 'forms.date.day'}}</option>\n          {{#for 1 31}}\n            <option value=\"{{$index}}\">{{$index}}</option>\n          {{/for}}\n        </select>\n      </span>\n    </span>\n\n  </span>\n\n  {{#if product.event_date.date_start}}\n    {{#if product.event_date.date_end}}\n      <span class=\"product-event-bounds\">{{lang 'forms.date.bounds' start_date=product.event_date.date_start end_date=product.event_date.date_end}}</span>\n    {{/if}}\n  {{/if}}\n</div>\n"}
