{"components/carousel":"\n<section class=\"carousel\">\n  {{#each carousel.slides}}\n    <figure class=\"carousel-item\">\n     {{#if url}}\n        <a class=\"carousel-item-image-link\" href=\"{{{url}}}\">\n          <img class=\"carousel-item-image\" src=\"{{image}}\" alt=\"{{{alt_text}}}\" />\n        </a>\n      {{else}}\n        <img class=\"carousel-item-image\" src=\"{{image}}\" alt=\"{{{alt_text}}}\" />\n      {{/if}}\n      {{#any\n        heading\n        text\n        button_text\n      }}\n        <figcaption class=\"carousel-item-info\">\n          {{#if heading}}\n            <h2 class=\"carousel-heading\">{{{heading}}}</h2>\n          {{/if}}\n          {{#if text}}\n            <p class=\"carousel-caption\">{{{text}}}</p>\n          {{/if}}\n          {{#if button_text}}\n            <a class=\"button carousel-button\" href=\"{{#if url}}{{url}}{{else}}#{{/if}}\">{{{button_text}}}</a>\n          {{/if}}\n        </figcaption>\n      {{/any}}\n    </figure>\n  {{/each}}\n\n  {{#if carousel.slides.length '>' 1}}\n    <div class=\"carousel-navigation\">\n      <span class=\"carousel-navigation-item carousel-previous\">{{> components/common/icons/left-arrow}}</span>\n      <span class=\"carousel-navigation-item carousel-next\">{{> components/common/icons/right-arrow}}</span>\n    </div>\n  {{/if}}\n</section>\n","components/common/icons/left-arrow":"<svg width=\"26px\" height=\"50px\" viewBox=\"0 0 26 50\" version=\"1.1\" xmlns=\"http://www.w3.org/2000/svg\" xmlns:xlink=\"http://www.w3.org/1999/xlink\" xml:space=\"preserve\">\r\n  <path d=\"M25.4 47.1L3.7 24.8 25.4 2.9c0.7-0.7 0.7-1.7 0-2.4 -0.7-0.7-1.7-0.7-2.4 0L0.6 23c-0.3 0.3-0.5 0.7-0.5 1.1l0 1.1c0 0.4 0.2 0.9 0.5 1.2l22.4 23c0.7 0.7 1.7 0.7 2.4 0C26.1 48.8 26.1 47.8 25.4 47.1z\"/>\r\n</svg>\r\n","components/common/icons/right-arrow":"<svg width=\"26px\" height=\"50px\" viewBox=\"0 0 26 50\" version=\"1.1\" xmlns=\"http://www.w3.org/2000/svg\" xmlns:xlink=\"http://www.w3.org/1999/xlink\" xml:space=\"p